source "${FIN_HOME}/fin-dev/bashrc"

# Manually call ssh_add_from_keychain.sh instead of doing this here:
# For fin laptop, keep id_rsa ssh key in keychain so it can be added with -K argument
# ssh-add -l | grep '.ssh/id_rsa ' > /dev/null || ssh-add -K

### JK aliasas
alias dotfiles='cd /Users/jennykortina/dotfiles'
alias developer='cd /Users/jennykortina/Developer'
alias sesher='cd /Users/jennykortina/Developer/sesher'
alias ngork='cd /Users/jennykortina/Developer && ./ngrok http fin.local:3000'
alias unity='cd /Users/jennykortina/Developer/unity/udemy-unity-2d-platformer'
alias fin='cd /Users/jennykortina/code/fin-analytics'
alias fa='./fa'

# rbenv
if which rbenv > /dev/null; then eval "$(rbenv init -)"; else echo rbenv not installed; fi

# pyenv
if which pyenv > /dev/null; then eval "$(pyenv init -)"; eval "$(pyenv virtualenv-init -)"; else echo pyenv not installed; fi

# nodenv
if which nodenv > /dev/null; then eval "$(nodenv init -)"; else echo nodenv not installed; fi
